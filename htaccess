
# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:SSL_REDIRECT
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE SSL_REDIRECT:SSL_REDIRECT

# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:bencod.es
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} ^bencod.es$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE SSL_REDIRECT:bencod.es
